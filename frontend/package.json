{
  "name": "frontend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack -p",
    "dev": "NODE_ENV=development node webpack-dev-server.js --max-old-space-size=512",
    "format": "prettier --write \"src/**/*.{js,vue}\"",
    "format:check": "prettier --list-different \"src/**/*.{js,vue}\"",
    "format:watch": "onchange --await-write-finish 0 \"src/**/*.{js,vue,json}\" -- sh -c 'clear; npx prettier --write {{changed}} && echo FORMAT OK'",
    "lint:check": "eslint -c .eslintrc.json src/**/*.{js,vue}",
    "lint:watch": "onchange \"src/**/*.{js,vue}\" -- sh -c 'clear; npx eslint -c .eslintrc.json {{changed}} && echo LINTER OK'"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "vue": "^2.6.10",
    "vue-click-outside": "^1.0.7",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/runtime": "^7.6.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "dotenv": "^8.1.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-vue": "^5.2.3",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "sass-loader": "^8.0.0",
    "vue-loader": "^15.7.1",
    "vue-svg-loader": "^0.12.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.39.3",
    "webpack-dev-server": "^3.8.0",
    "write-file-webpack-plugin": "^4.5.1"
  }
}
