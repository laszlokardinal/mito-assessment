{
  "name": "frontend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack -p",
    "dev": "NODE_ENV=development node webpack-dev-server.js --max-old-space-size=512",
    "format": "prettier --write \"src/**/*.{js,vue}\"",
    "format:check": "prettier --list-different \"src/**/*.{js,vue}\"",
    "format:watch": "onchange --await-write-finish 0 \"src/**/*.{js,vue,json}\" -- sh -c 'clear; npx prettier --write {{changed}} && echo FORMAT OK'",
    "lint:check": "eslint -c .eslintrc.json src/**/*.{js,vue}",
    "lint:watch": "onchange \"src/**/*.{js,vue}\" -- sh -c 'clear; npx eslint -c .eslintrc.json {{changed}} && echo LINTER OK'",
    "test": "NODE_ENV=test mocha-webpack --webpack-config webpack.config.js --require init.test.js src/**/*.test.js",
    "test:coverage": "NODE_ENV=test nyc mocha-webpack --webpack-config webpack.config.js --require init.test.js src/**/*.test.js",
    "test:watch": "NODE_ENV=test nyc mocha-webpack --webpack-config webpack.config.js --require init.test.js src/**/*.test.js --watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/preset-env": "^7.6.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^5.2.0",
    "chai": "^4.2.0",
    "chai-diff": "^1.0.1",
    "css-loader": "^3.2.0",
    "dotenv": "^8.1.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-vue": "^5.2.3",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jsdom": "^15.1.1",
    "jsdom-global": "^3.0.2",
    "mocha": "^6.2.0",
    "mocha-webpack": "^2.0.0-beta.0",
    "node-sass": "^4.12.0",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "pretty": "^2.0.0",
    "pretty-format": "^24.9.0",
    "sass-loader": "^8.0.0",
    "sinon": "^7.4.2",
    "sinon-chai": "^3.3.0",
    "vue-loader": "^15.7.1",
    "vue-svg-loader": "^0.12.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0",
    "webpack-node-externals": "^1.7.2",
    "write-file-webpack-plugin": "^4.5.1"
  }
}
